{
  "name": "k-platform",
  "version": "5.1.0",
  "scripts": {
    "start": "ng serve app -o --no-hmr",
    "start-no-open": "ng serve app --no-hmr",
    "start-no-reload": "ng serve app -o --no-live-reload --no-hmr",
    "start-external": "ng serve app -o --host 0.0.0.0 --no-hmr",
    "start-external-no-reload": "ng serve app --host 0.0.0.0 --no-live-reload --no-hmr",
    "shared": "ng build shared",
    "prod": "NG_BUILD_OPTIMIZE_CHUNKS=1 ng build app --configuration production",
    "ng": "ng",
    "build": "ng build app",
    "test": "ng test",
    "lint": "ng lint",
    "builder": "tsc -p builders/tsconfig.builders.json",
    "build:stats": "ng build app --stats-json",
    "analyze": "webpack-bundle-analyzer dist/app/stats.json",
    "plugins": "node builders/script-builder.js projects/plugins/plugins.json --all --prod",
    "plugins-dev": "node builders/script-builder.js projects/plugins/plugins.json --all",
    "update-icons": "cd ./projects/shared/src/lib/services/icon/scripts/ && node getIcons.js",
    "e2e": "ng e2e",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "prettier": {
